<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shahajalal.flexiloadapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.Dark">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AfterLoginPage" />
        <activity android:name=".FlexiLoad" />
        <activity android:name=".Bkash" />
        <activity android:name=".Rocket" />
        <activity android:name=".Mcash" />
        <activity android:name=".Ucash" />
        <activity android:name=".PrepaidCard" />
        <activity android:name=".Offer" />
        <activity android:name=".Addreseller" />
        <activity android:name=".History" />
        <activity android:name=".Resellers"></activity>
    </application>

</manifest>